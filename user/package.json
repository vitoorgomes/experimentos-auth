    "bcrypt": "5.0.0",
    "mongoose": "5.10.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.2"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.0.27",
    "dotenv": "8.2.0",
    "ts-node": "8.10.2",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
